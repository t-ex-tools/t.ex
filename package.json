{
  "name": "t.ex-mv3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "concurrently \"vue-cli-service build --watch --no-clean\" \"npm run copy:css:light\" \"npm run copy:css:dark\"",
    "build": "concurrently \"vue-cli-service build\" \"npm run copy:css:light\" \"npm run copy:css:dark\"",
    "lint": "vue-cli-service lint",
    "copy:css:dark": "ncp node_modules/bootstrap-dark-5/dist/css/bootstrap-night.css dist/css/bootstrap-night.css",
    "copy:css:light": "ncp node_modules/bootstrap/dist/css/bootstrap.min.css dist/css/bootstrap.min.css"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "bootstrap-dark-5": "^1.1.3",
    "bootstrap-icons": "^1.8.1",
    "core-js": "^3.6.5",
    "vue": "^3.0.0",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "concurrently": "^7.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "ncp": "^2.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
